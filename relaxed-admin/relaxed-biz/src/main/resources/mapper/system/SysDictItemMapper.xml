<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.relaxed.boot.biz.system.mapper.SysDictItemMapper">

	<resultMap id="sysDictItemMap" type="com.relaxed.boot.common.system.model.entity.SysDictItem">
  		<id property="id" column="id"/>
    		<result property="dictCode" column="dict_code"/>
    		<result property="value" column="value"/>
    		<result property="name" column="name"/>
    		<result property="status" column="status"/>
    		<result property="attributes" column="attributes" typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
    		<result property="sort" column="sort"/>
    		<result property="remarks" column="remarks"/>
    		<result property="deleted" column="deleted"/>
    		<result property="createBy" column="create_by"/>
    		<result property="updateBy" column="update_by"/>
    		<result property="createdTime" column="created_time"/>
    		<result property="updatedTime" column="updated_time"/>
      </resultMap>

	<sql id="Base_Column_List">
		id,        
		dict_code,        
		value,        
		name,        
		status,        
		attributes,        
		sort,        
		remarks,        
		deleted,        
		create_by,        
		update_by,        
		created_time,        
		updated_time        
	</sql>

	<sql id="Base_Alias_Column_List">
		sdi.id,        
		sdi.dict_code,        
		sdi.value,        
		sdi.name,        
		sdi.status,        
		sdi.attributes,        
		sdi.sort,        
		sdi.remarks,        
		sdi.deleted,        
		sdi.create_by,        
		sdi.update_by,        
		sdi.created_time,        
		sdi.updated_time        
	</sql>
</mapper>
